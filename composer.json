{
    "name": "simplygoodwork/start",
    "description": "Craft CMS, Alpine.js and Tailwind CSS",
    "license": "MIT",
    "homepage": "https://simplygoodwork.com/",
    "type": "project",
    "support": {
        "email": "dev@simplygoodwork.com",
        "issues": "https://github.com/simplygoodwork/start/issues"
    },
    "require": {
        "aodihis/video-utils": "3.0.2",
        "craftcms/ckeditor": "4.11.0",
        "craftcms/cms": "5.9.6",
        "nystudio107/craft-cookies": "5.0.0",
        "nystudio107/craft-vite": "^4.0",
        "nystudio107/craft-retour": "5.0.13",
        "nystudio107/craft-seomatic": "5.1.20",
        "nystudio107/craft-vite": "5.0.1",
        "putyourlightson/craft-dashboard-begone": "3.0.0",
        "putyourlightson/craft-sprig": "3.7.1",
        "servd/craft-asset-storage": "4.2.3",
        "spicyweb/craft-neo": "5.5.4",
        "verbb/formie": "3.1.10",
        "verbb/hyper": "2.3.0",
        "verbb/navigation": "3.0.16",
        "vlucas/phpdotenv": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "modules\\runwaymodule\\": "modules/runwaymodule/src/",
            "modules\\expansion\\": "modules/expansion/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            "@composer dump-autoload -o"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}
